{
    "name": "oort-cloud-reverie",
    "displayName": "oort-cloud-reverie",
    "description": "A dark bluish theme for VS Code. Created with C# TypeScript JavaScript CSS, Node.js and ASP.NET in mind.",
    "license": "MIT",
    "publisher": "W3Dojo",
    "repository": "https://github.com/AjayChambers/oort-cloud-reverie.git",
    "version": "1.2.0",
    "main": "src/main.js",
    "engines": {
        "vscode": "^1.100.0"
    },
    "categories": [
        "Themes"
    ],
    "activationEvents": [],
    "contributes": {
        "themes": [
            {
                "label": "Dark Inner Galactic Space",
                "uiTheme": "vs-dark",
                "path": "./themes/dark-inner-galactic-space-color-theme.json"
            },
            {
                "label": "Deep Blue Galactic Glow",
                "uiTheme": "vs-dark",
                "path": "./themes/deep-blue-galactic-glow-color-theme.json"
            }
        ]
    },
    "scripts": {
        "login": "vsce login w3dojo",
        "build": "vsce package --out ./out",
        "publish": "vsce publish minor",
        "publish-fix": "vsce publish patch"
    }
}
