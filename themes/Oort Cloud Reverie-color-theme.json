    "tokenColors": [
        /*
        *************************************************************
                      -- DEFAULT SYNTAX HIGHLIGHTING --
        ******************************x******************************/
        {
            "name": "Default Syntax Color",
            "scope": "source",
            "settings": {
                "foreground": "#C4C4C4"
            }
        },
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#4D709D"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#98A0BB"
            }
        },

        {
            "name": "String Primitive",
            "scope": ["string.quoted", "punctuation.definition.string"],
            "settings": {
                "foreground": "#c99b98"
            }
        },

        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#4090FF", //"#8BBFFF"
                "fontStyle": ""
            }
        },
        {
            "name": "Control Flow Keywords",
            "scope": "keyword",
            "settings": {
                "foreground": "#4090FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Unary Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#4090FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable Default",
            "scope": ["variable", "entity.name.variable"],
            "settings": {
                "foreground": "#C8C8C8"
            }
        },

        {
            "name": "Variable Parameter",
            "scope": "entity.name.variable.parameter",
            "settings": {
                "foreground": "#cfb371"
            }
        },
        {
            "name": "Functions, Methods",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#88C0F4"
            }
        },
        {
            "name": "Variable, I/O Variable", // Example: "ArgV"
            "scope": "variable.other.readwrite",
            "settings": {
                "foreground": "#C4C4C4",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Types, Namespaces, Objects",
            "scope": [
                "entity.name.type",
                "entity.name.type.namespace",
                "variable.other.object"
            ],
            "settings": {
                "foreground": "#AACE88"
            }
        },

        {
            "name": "Classes",
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#ADD58D"
            }
        },

        /*
      
      


      *------------------------------------------*
         -- JSON & JSONC DATA FORMATS --
      *------------------------------------------*/
        {
            "name": "JSON > Default Syntax Color",
            "scope": ["source.json"],
            "settings": {
                "foreground": "#96CCF5"
            }
        },
        {
            "name": "JSON > Properties", // aka keys
            "scope": ["source.json support.type.property-name"],
            "settings": {
                "foreground": "#99d0fa"
            }
        },
        {
            "name": "JSON > String Values",
            "scope": ["source.json string"],
            "settings": {
                "foreground": "#ADD58D"
            }
        }
    ]
